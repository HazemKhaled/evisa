{
  "name": "gettravelvisa-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "dev": "pnpm --parallel --filter \"./apps/*\" dev",
    "dev:website": "pnpm --filter @repo/website dev",
    "dev:admin": "pnpm --filter @repo/admin dev",
    "build": "pnpm --recursive --filter \"./apps/*\" build",
    "build:website": "pnpm --filter @repo/website build",
    "build:admin": "pnpm --filter @repo/admin build",
    "start": "pnpm --parallel --filter \"./apps/*\" start",
    "lint": "pnpm --recursive lint",
    "lint:fix": "pnpm --recursive lint:fix && pnpm format",
    "type-check": "pnpm --recursive type-check",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "pnpm --recursive test",
    "test:watch": "pnpm --recursive test:watch",
    "test:coverage": "pnpm --recursive test:coverage",
    "test:ci": "pnpm --recursive test:ci",
    "deploy:website": "pnpm run --filter @repo/website deploy",
    "deploy:admin": "pnpm run --filter @repo/admin deploy",
    "db:generate": "pnpm --filter @repo/database db:generate",
    "db:migrate": "pnpm --filter @repo/database db:migrate",
    "db:push": "pnpm --filter @repo/database db:push",
    "db:studio": "pnpm --filter @repo/database db:studio",
    "prepare": "husky",
    "bmad:refresh": "pnpx bmad-method install -f -i codex",
    "bmad:list": "pnpx bmad-method list:agents",
    "bmad:validate": "pnpx bmad-method validate"
  },
  "dependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.39.1",
    "@next/eslint-plugin-next": "15.4.7",
    "@types/node": "24.10.0",
    "@types/react": "19.1.17",
    "@types/react-dom": "19.1.11",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "eslint": "9.39.1",
    "eslint-config-next": "15.4.7",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-i18next": "6.1.3",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "tailwindcss": "4.1.16",
    "typescript": "5.9.3"
  }
}
