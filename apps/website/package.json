    "@repo/ui": "workspace:*",
    "@repo/utils": "workspace:*",
    "@sentry/nextjs": "10.20.0",
    "i18next": "25.6.0",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-resources-to-backend": "1.2.1",
    "import-in-the-middle": "1.15.0",
    "lucide-react": "0.546.0",
    "next": "15.4.7",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-i18next": "16.1.0"
