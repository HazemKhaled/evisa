    "@types/node": "22.18.11",
    "@types/react": "19.1.17",
    "@types/react-dom": "19.1.11",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "shadcn": "3.4.2",
    "tailwindcss": "4.1.14",
    "ts-jest": "29.4.5",
    "typescript": "5.9.3",
    "wrangler": "4.43.0"
